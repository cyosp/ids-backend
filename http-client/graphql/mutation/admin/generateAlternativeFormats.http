< {%
    request.variables.set("admin-access-token", client.global.get("admin-access-token"))
%}
GRAPHQL {{urlPrefix}}/graphql
Authorization: Bearer {{admin-access-token}}

mutation generateAlternativeFormats {
  generateAlternativeFormats(directory: "diectory", forceThumbnailGeneration: false) {
    id
    __typename
    name
    urlPath
    previewUrlPath
    thumbnailUrlPath
  }
}
